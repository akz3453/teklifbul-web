{
  "name": "teklifbul-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npm config set legacy-peer-deps true",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "api": "tsx server/src/index.ts",
    "dev:api": "tsx watch server/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:setup": "npm i -D @firebase/rules-unit-testing@^3.0.4",
    "emulators": "firebase emulators:start",
    "backfill:categories": "node scripts/backfill-categories.js",
    "backfill:satfk": "node scripts/backfill-satfk.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "fast-levenshtein": "^3.0.0",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "firebase-tools": "^14.20.0",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^3.11.174",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "index.html",
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "assets/js/**/*.js",
      "!assets/js/vendor/**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
