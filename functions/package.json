{
  "name": "teklifbul-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for Teklifbul",
  "main": "index.js",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.6.0",
    "teklifbul-web": "file:.."
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
